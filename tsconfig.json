{
    "compilerOptions": {
        "sourceMap": true,
        "allowJs": true,
        "target": "ES2020",
        "strict": true,
        "module": "esnext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "jsx": "react-jsx",
        "experimentalDecorators": true
    },
    "exclude": [
        "node_modules",
        "dist",
        "poi.config.js",
        "./**/*.js",
        "./**/*.jsx"
    ]
}
